//输入三个数，输出 同样三个数 从大到小排列
#define _CRT_SECURE_NO_WARNINGS


#include<stdio.h>
#include<string.h>
int main()
{
	int a = 0;
	int b = 0;
	int c = 0;
	scanf("%d%d%d", &a, &b, &c);
	if (a < b)
		
	{
		int tmp = a;
		a = b;
		b = tmp;
	}
	if (a < c)
	{
		int tmp = a;
		a = c;
		c = tmp;
	}
	if (b < c)
	{
		int tmp = b;
		b = c;
		c = tmp;
	}
	printf("%d %d %d\n", a, b, c);
	return 0;
}

//作业：打印1-100以内 3的倍数
#define _CRT_SECURE_NO_WARNINGS
#include<stdio.h>

int main()
{
	int i = 0;
	for (i = 1; i <= 100; i++)
	{
		if (i % 3 == 0)
		{
			printf("%d ",i);
		}
	}
	return 0;
}

//作业：给定2个数，求这两个数最大公约数
#define _CRT_SECURE_NO_WARNINGS
#include<stdio.h>

int main()
{
	int m = 0;
    int n = 0;
	int r = 0;
	scanf("%d %d", &m, &n);
	while (r=m%n)
	{
		m = n;
		n = r;
		
	}
	printf("%d\n", n);

	return 0;
}
